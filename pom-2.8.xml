<?xml version="1.0"?>
<project xmlns="http://maven.apache.org/POM/4.0.0">
  <modelVersion>4.0.0</modelVersion>
  <packaging>pom</packaging>
  <parent>
        <groupId>org.sakaiproject.purepoms</groupId>
        <artifactId>sakai-standard-tool</artifactId>
        <version>2.8.6</version>
    </parent>
  <name>sakai-reports</name>
  <groupId>org.sakaiproject.reports</groupId>
  <artifactId>reports</artifactId>
  <version>2.10-SNAPSHOT</version>
  <organization>
    <name>sakai</name>
    <url>http://sakaiproject.org/</url>
  </organization>
  <inceptionYear>2003</inceptionYear>
  <scm>
    <connection>scm:svn:https://source.sakaiproject.org/svn/reports/trunk</connection>
    <developerConnection>scm:svn:https://source.sakaiproject.org/svn/reports/trunk</developerConnection>
    <url>https://source.sakaiproject.org/svn/reports/trunk</url>
  </scm>
  <modules>
    <module>reports-api/api</module>
    <module>reports-impl/impl</module>
    <module>reports-impl/pack</module>
    <module>reports-tool/tool</module>
    <module>reports-util/util</module>
    <module>reports-assembly/assembly</module>
  </modules>
  <properties>
        <url.localsite>scp://source.sakaiproject.org/var/www/html/release/reports/${project.version}</url.localsite>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <sakai.metaobj.group>org.sakaiproject</sakai.metaobj.group>
        <sakai.metaobj.version>2.8.2</sakai.metaobj.version>
        <sakai.osp.group>org.sakaiproject.osp</sakai.osp.group>
        <sakai.osp.version>2.8.2</sakai.osp.version>
        <sakai.quartz.group>org.opensymphony.quartz</sakai.quartz.group>
        <sakai.quartz.version>1.6.6</sakai.quartz.version>
        <sakai.scheduler.version>2.8.3</sakai.scheduler.version>
        <sakai.jsf.version>2.8.4</sakai.jsf.version>
        <sakai.taggable.group>org.sakaiproject</sakai.taggable.group>
        <sakai.taggable.version>2.8.2</sakai.taggable.version>
    </properties>
  <build>
    <resources/>
  </build>
  <dependencyManagement>
    <dependencies>
      <dependency>
        <groupId>${sakai.metaobj.group}</groupId>
        <artifactId>sakai-metaobj-api</artifactId>
        <version>${sakai.metaobj.version}</version>
        <scope>provided</scope>
      </dependency>
      <dependency>
        <groupId>${sakai.osp.group}</groupId>
        <artifactId>osp-common-api</artifactId>
        <version>${sakai.osp.version}</version>
        <scope>provided</scope>
      </dependency>
      <dependency>
        <groupId>${sakai.osp.group}</groupId>
        <artifactId>osp-matrix-api</artifactId>
        <version>${sakai.osp.version}</version>
        <scope>provided</scope>
      </dependency>
      <dependency>
        <groupId>${sakai.taggable.group}</groupId>
        <artifactId>sakai-taggable-api</artifactId>
        <version>${sakai.taggable.version}</version>
        <scope>provided</scope>
      </dependency>
      <dependency>
        <groupId>org.sakaiproject.scheduler</groupId>
        <artifactId>scheduler-api</artifactId>
        <version>${sakai.scheduler.version}</version>
        <scope>provided</scope>
      </dependency>
      <dependency>
        <groupId>org.sakaiproject.scheduler</groupId>
        <artifactId>scheduler-component-shared</artifactId>
        <version>${sakai.scheduler.version}</version>
        <scope>provided</scope>
      </dependency>
      <dependency>
        <groupId>org.sakaiproject.reports</groupId>
        <artifactId>reports-api</artifactId>
        <version>${project.version}</version>
        <scope>provided</scope>
      </dependency>
      <dependency>
         <groupId>${sakai.quartz.group}</groupId>
         <artifactId>quartz</artifactId>
         <version>${sakai.quartz.version}</version>
         <scope>provided</scope>
    </dependency>
    <dependency>
      <groupId>javax.servlet.jsp</groupId>
      <artifactId>jsp-api</artifactId>
      <version>2.0</version>
    </dependency>
    </dependencies>
  </dependencyManagement>
  <repositories>
    <repository>
        <id>sonatype-nexus-snapshots</id>
        <name>Sonatype Nexus Snapshots</name>
        <url>https://oss.sonatype.org/content/repositories/snapshots</url>
        <releases>
        <enabled>false</enabled>
        </releases>
        <snapshots>
            <enabled>true</enabled>
            </snapshots>
        </repository>
        <repository>
            <id>sakai-maven</id>
            <name>Sakai Maven Repo</name>
            <layout>default</layout>
            <url>http://source.sakaiproject.org/maven2</url>
            <releases>
                <enabled>true</enabled>
            </releases>
            <snapshots>
                <enabled>false</enabled>
            </snapshots>
        </repository>
        <repository>
            <id>default</id>
            <name>Maven Repository Switchboard</name>
            <layout>default</layout>
            <url>http://repo1.maven.org/maven2</url>
            <releases>
                <enabled>true</enabled>
            </releases>
            <snapshots>
                <enabled>false</enabled>
            </snapshots>
        </repository>
    </repositories>

</project>
